    "brew-bump": "BIT_VERSION=$(cat ./package.json | jq .version -r) && brew bump-formula-pr bit --url='https://registry.npmjs.org/bit-bin/-/bit-bin-${BIT_VERSION}.tgz' --message='version bump'",
    "brew-bump:dry-run": "BIT_VERSION=$(cat ./package.json | jq .version -r) && brew bump-formula-pr bit --url='https://registry.npmjs.org/bit-bin/-/bit-bin-${BIT_VERSION}.tgz' --message='version bump' --dry-run",
    "build": "run-s build:src",
    "build-centos-image": "docker build ./scripts/linux/centos -t centos-rpm",
    "build-debian-image": "docker build ./scripts/linux/debian -t debian-deb",
    "build-debug": "babel src -d dist --source-maps --extensions \".ts\"",
    "build:ext": "tsc -p tsconfig.extensions.json",
    "build:pjson": "cp package.json dist/",
    "build:src": "babel src -d dist --verbose --extensions \".ts,.tsx,.js\" --copy-files",
    "build:types": "./scripts/build-types.bash",
    "check-types": "tsc",
    "clean": "rm -rf dist && rm -rf types",
    "dev-link": "node ./scripts/establish-dev-link.js $1",
    "doc-gen": "node ./scripts/doc-generator.js",
    "e2e-test": "mocha --require ./babel-register ./e2e/**/*.e2e*.ts",
    "e2e-test-circle": "mocha --require ./babel-register --reporter mocha-circleci-reporter ./e2e/**/*.e2e*.ts",
    "e2e-test:appveyor-debug-example": "set npm_config_debug=true && mocha --require ./babel-register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.2.ts --reporter-options appveyorBatchSize=1",
    "e2e-test:appveyor1": "mocha --require ./babel-register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.1.ts 2> output.err",
    "e2e-test:appveyor2": "mocha --require ./babel-register --reporter mocha-appveyor-reporter ./e2e/**/*e2e.2.ts 2> output.err",
    "e2e-test:appveyor3": "mocha --require ./babel-register --reporter mocha-appveyor-reporter ./e2e/**/*.e2e.3.ts 2> output.err",
    "e2e-test:debug": "npm run e2e-test --debug --keep-envs",
    "e2e-test:ssh": "npm run e2e-test --with-ssh",
    "e2e-test:ssh-debug": "npm run e2e-test --with-ssh --debug --keep-envs",
    "format": "prettier-eslint \"{src,e2e}/**/*.{ts,tsx}\" --write",
    "lint": "tsc && eslint \"{src,e2e}/**/*.{ts,tsx}\"",
    "lint-circle": "eslint \"{src,e2e}/**/*.{ts,tsx}\" --format junit -o junit/eslint-results.xml",
    "lint:html": "eslint \"{src,e2e}/**/*.{ts,tsx}\" --format html -o eslint-report.html",
    "lint:table": "eslint \"{src,e2e}/**/*.{ts,tsx}\" --format table",
    "mocha-circleci": "mocha --require ./babel-register --reporter mocha-circleci-reporter",
    "performance-test": "mocha --require ./babel-register ./e2e/performance/*.performance*.ts",
    "performance-test-circle": "mocha --require ./babel-register --reporter mocha-circleci-reporter ./e2e/performance/*.performance*.ts",
    "pkg": "pkg bin/bit.js --targets node10 --out-path releases/ --options --no-warnings --config package.json",
    "pkg:all": "pkg bin/bit.js --targets node10-macos-x64,node10-win-x64,node10-linux-x64 --out-path releases/ --options --no-warnings --config package.json",
    "pkg:linux": "pkg bin/bit.js --targets node10-linux-x64 --out-path releases/linux --options --no-warnings --config package.json",
    "pkg:mac": "pkg bin/bit.js --targets node10-macos-x64 --out-path releases/mac --options --no-warnings --config package.json",
    "pkg:windows": "pkg bin/bit.js --targets node10-win-x64 --out-path releases/windows --options --no-warnings --config package.json",
    "pre-release": "gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos --prerelease",
    "pre-release:circle": "gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos -y --prerelease",
    "pre-release:inc-pack": "npm run pkg:all && npm run pre-release",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run clean && npm run build && npm run build:types",
    "prettier:check": "prettier --list-different \"{src,e2e}/**/*.{ts,tsx}\"",
    "release": "gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos",
    "release:circle": "gh-release -a releases/bit-bin-linux,releases/bit-bin-win.exe,releases/bit-bin-macos -y",
    "release:inc-pack": "npm run pkg:all && npm run release",
    "test": "mocha --require ./babel-register './src/**/*.spec.ts'",
    "test-circle": "mocha --require ./babel-register --reporter mocha-circleci-reporter './src/**/*.spec.ts'",
    "test:appveyor": "mocha --require ./babel-register --reporter mocha-appveyor-reporter ./src/**/*.spec.ts",
    "ts-coverage": "type-coverage",
    "types-wip": "tsc -p tsconfig.types-clean.json --outfile dist/index.d.ts",
    "watch": "babel --watch src -d dist --verbose --source-maps --extensions \".ts,.tsx,.js\" --copy-files"
  },
  "version": "14.8.8"
